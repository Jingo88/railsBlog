  <%= form_for @entry, url: {action: 'create'} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br /><br />

  <%= f.label :author %>
  <%= f.text_field :author %>
  <br /><br />

  <%= f.label :content %>
  <%= f.text_area :content %>
  <br /><br />

  <%= f.submit %>
  <% end %>

  <%= link_to 'cancel', entries_path %>